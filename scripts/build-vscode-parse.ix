# this script generates ./vscode-inox/vscode-inox.wasm

manifest {
    permissions: {
        use: {
            commands: {
                "go": {}
            }
        }
    }
}

print "build vscode-parse"


build_cmd_env = {
    GOARCH: "wasm"
    GOOS: "js"
}

assign result err = ex(
    ..200s --env=build_cmd_env 
    #go #build -x -v -o ./vscode/parse.wasm ./vscode/parse/main.go
)


if err? {
    print "failed:\n"   
}
print $result
