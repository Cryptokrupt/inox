manifest {}


year = 2020y-UTC
date = 2020y-1mt-1d-UTC # first of January
datetime = 2020y-10mt-5d-5h-4m-CET

for datelike in [year, date, now(), datetime] {
    print(datelike)
}

# the two same years in different locations are not equal.
print "(2020y-UTC == 2020y-America/Los_Angeles):" (2020y-UTC == 2020y-America/Los_Angeles)

# a year is never equal to a date.
print "(2020y-UTC == 2020y-1mt-1d-UTC):" (2020y-UTC == 2020y-1mt-1d-UTC)
