manifest {
    permissions: {
        read: %https://localhost:8080/...
        provide: https://localhost:8080
    }
}


fn handle(rw %http.resp_writer, r %http.req){
    rw.write_json({ aa: 1 })
}

server = http.Server!(https://localhost:8080, Mapping {
    /a => "a"
    %/... => handle
})

body = http.read!(https://localhost:8080/a, --insecure)
print $body

body = http.read!(https://localhost:8080/, --insecure)
print $body

server.close()