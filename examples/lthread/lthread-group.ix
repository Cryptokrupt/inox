const (
    DOMAIN = https://jsonplaceholder.typicode.com
)

manifest {
    permissions: {
        read: DOMAIN
        create: {
            threads: {}
        }
    }
}

req_group = LThreadGroup()

for (1 .. 2) {
    go {
        group: req_group 
        globals: {DOMAIN: DOMAIN}
    } do {
        return read!(https://jsonplaceholder.typicode.com/posts/1)
    } 
}

results = req_group.wait_results!()
print $results
